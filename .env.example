# KidsSmart+ environment template

# Core
APP_ENV=development
SECRET_KEY=anythingRandom123
JWT_ALGO=HS256

# Database
DB_URL=postgresql+psycopg2://postgres:postgres@db:5432/kidssmart
DB_URL_SQLITE=sqlite+aiosqlite:///./kidssmart_dev.db

# Redis / Celery
REDIS_URL=redis://redis:6379/0

# API Keys
EVENTBRITE_TOKEN=
MEETUP_TOKEN=
SERPAPI_KEY=

# Feature flags
ENABLE_PLAYWRIGHT=false
ENABLE_EMBEDDINGS=false

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8501

# Map/Geo
MAPS_TOKEN=

# Rate limit
RATE_LIMIT_PER_MIN=120
CACHE_TTL=60

# Auth
ADMIN_USERNAME=admin@example.com
ADMIN_PASSWORD=Passw0rd!

# Dedupe
NEARDUP_THRESHOLD=0.85

# Geocoding
GEOCODING_ENABLED=false

# Dashboard API connection (use service name for Docker, localhost for local dev)
API_BASE=http://api:8000

